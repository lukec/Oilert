#!/usr/bin/env perl
use strict;
use warnings;
use IO::Socket;

my $host = 'data.aishub.net:4275';
my $sock = IO::Socket::INET->new( PeerAddr => $host );
die "Could not connect to $host" unless $sock->connected;
print "Connected\n" if $sock->connected;

while (my $line = $sock->getline) {
    print $line;
}
